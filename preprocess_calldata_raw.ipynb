    "for year_idx, year in enumerate(years) :\n",
    "\n",
    "    year = str(year)\n",
    "    data_dir = os.path.abspath(os.path.join(os.getcwd(), 'rawdata', target, year))\n",
    "    filelist = os.listdir(data_dir)\n",
    "    filelist.sort()\n",
    "    \n",
    "    for month_idx, filename in enumerate(filelist) : \n",
    "        print(filename)\n",
    "        if filename != '.DS_Store' :\n",
    "        \n",
    "            path = os.path.abspath(os.path.join(data_dir, filename))\n",
    "            df_tomerge = pd.read_csv(path)\n",
    "\n",
    "            # 총건수로 이름 통일\n",
    "            candidates = ['총_건수', '이용건수', '통화건수', '통화량']\n",
    "            for candidate in candidates : \n",
    "                try : \n",
    "                    df_tomerge = df_tomerge.rename(columns={candidate: '총건수'})\n",
    "                except Exception as e_total_amount : \n",
    "                    print('e_total_amount : ', e_total_amount)\n",
    "\n",
    "            # 0번째 column '일자'로 이름 통일\n",
    "            #df_tomerge = df_tomerge.rename(columns={ df_tomerge.columns[0]: '일자' }, inplace = True)\n",
    "\n",
    "            # 일자 format 통일\n",
    "            try : \n",
    "                df_tomerge['일자'] = df_tomerge['일자'].str.replace('년', '')\n",
    "                df_tomerge['일자'] = df_tomerge['일자'].str.replace('월', '')\n",
    "                df_tomerge['일자'] = df_tomerge['일자'].str.replace('일', '')\n",
    "            except Exception as e_date :\n",
    "                print('e_date : ', e_date)\n",
    "\n",
    "            # 시간 format 통일\n",
    "            try : \n",
    "                df_tomerge['시간대'] = df_tomerge['시간대'].str.replace('시간대', '')\n",
    "            except Exception as e_time : \n",
    "                print('e_time : ', e_time)\n",
    "\n",
    "            # 일자 datetime으로 변경\n",
    "            try :     \n",
    "                df_tomerge[df_tomerge.columns[0]] = pd.to_datetime(df_tomerge[df_tomerge.columns[0]], format='%Y%m%d')\n",
    "            except Exception as e :\n",
    "                print(e)\n",
    "\n",
    "            # 첫번째 인 경우\n",
    "            if year_idx == 0 and month_idx == 0 :\n",
    "                df = df_tomerge.copy()\n",
    "\n",
    "            # year_idx <= 3 : 2013, 2014, 2015 인 경우\n",
    "            elif year_idx <= 2 :\n",
    "                df = pd.concat([df, df_tomerge], ignore_index=True)\n",
    "            # year_idx == 4 and month_idx <= 5 : 2016년이면서 5월 이전(5월 포함)인 경우\n",
    "            elif year_idx == 3 and month_idx <= 4 :\n",
    "                df = pd.concat([df, df_tomerge], ignore_index=True)\n",
    "\n",
    "            # 2016년 6월 이후(6월 포함) 이 ㄴ경우\n",
    "            else :\n",
    "\n",
    "                df_temp = df_tomerge.groupby(['일자', '요일', '시간대', '업종'])['총건수'].sum()\n",
    "                df_temp = df_temp.to_frame()\n",
    "                df_temp = df_temp.reset_index(level=[0,1,2])\n",
    "                df_temp = df_temp.reset_index(drop=False)\n",
    "\n",
    "                df_tomerge = df_temp.copy()\n",
    "                test = df_temp.copy()     \n",
    "\n",
    "                '''\n",
    "                if year_idx == 3 and month_idx == 5 :\n",
    "                    df2 = df_tomerge.copy()\n",
    "                else : \n",
    "                    df2 = pd.concat([df2, df_tomerge], ignore_index=True)\n",
    "                '''\n",
    "                \n",
    "                df = pd.concat([df, df_tomerge], ignore_index=True)\n",
    "                display(df_tomerge.head())\n",
